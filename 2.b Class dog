class Dog:
  def __init__(self,name="Unknown",size=None,breed='Unknown',dob=None):
    self.__name=name
    self.size=size
    self.breed=breed
    self.dob=dob
    self.bark()
    self.get_name()
    self.dog_years()

  def bark(self):
    print("WOO!")
  def get_name(self):
    print(f"dog name ={self.__name}")
  def set_name(self,name):
    self.__name=name

  def dog_years(self):
    self.dob=(2023-self.dob)*7
    print(f"{self.__name} age in dog years is {self.dob}")

d1=Dog("sss","med","Husky",2008)
d1.set_name("RRR")
d1.get_name()
d1.dog_years()
