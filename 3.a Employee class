class Employee:
  def __init__(self,name,salary):
    self.name=name
    self.__salary=salary

  def work(self):
      print(f"{self.name} is working.")

  def show(self):
      print(f"Employee: {self.name}, Salary: ${self.__salary}")

emp1=Employee("Ram",500000)
emp1.work()
emp1.show()
print(f"Employee Name:{emp1.name} has salary:{emp1._Employee__salary}") #name mangling
