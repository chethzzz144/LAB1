class Student:
  pass
class Marks:
  pass
student=Student()
marks=Marks()
is_instance_std=isinstance(student,Student)
is_instance_mar=isinstance(marks,Marks)
print("is student instance of Student:",is_instance_std)
print("is marks instance of Marks:",is_instance_mar)
is_subclass_std=issubclass(Student,object)
is_subclass_mar=issubclass(Marks,object)
print(is_subclass_std)
print(is_subclass_std)
